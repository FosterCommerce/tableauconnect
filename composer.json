{
    "name": "fostercommerce/tableauconnect",
    "description": "Tableau integration plugin for Craft",
    "type": "craft-plugin",
    "version": "1.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "tableau connect"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Foster Commerce",
            "homepage": "https://www.fostercommerce.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
          "fostercommerce\\tableauconnect\\": "src/"
        }
    },
    "extra": {
        "name": "Tableau Connect",
        "handle": "tableauconnect",
        "hasCpSettings": true,
        "hasCpSection": false,
        "components": {
            "link": "fostercommerce\\tableauconnect\\services\\Link",
            "visualization": "fostercommerce\\tableauconnect\\services\\Visualization"
        },
        "class": "fostercommerce\\tableauconnect\\Plugin"
    }
}
